	IMG:128x128/apps/arandr.png			arandr			exec arandr
	IMG:128x128/apps/text-editor.png		vim			exec xterm -e vim
IMG:128x128/apps/preferences-system-windows.png		minimized windows	$ shodc list -l | awk 'BEGIN {FS = "\t"} /^...m/ && $4 == $6 {print substr($7, 1, 16) "twin minimize " $6}'
IMG:128x128/apps/screenshooter.png			take screenshot		exec scrot select
