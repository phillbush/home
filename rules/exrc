set noignorecase
set nolist
set magic
set mesg
set number
set ruler
set shiftwidth=8
set showmatch
set showmode
set tabstop=8
set tildeop
set windowname
set wraplen=72
set leftright
set extended
set iclower
set searchincr
map ZZ m
map ) 0
map 0 ^
map - $
map W b
map m '
map M `
map Y y$
