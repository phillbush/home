Apps
	Draw Terminal					exec drawterm
	KEY:A-T		Open Terminal at $HOME		exec xterm
	KEY:A-S-T	Open Terminal Here		exec samedir
	Take Screenshot					exec scrot select
	RUN:	KEY:A-space	Run			exec xfilter -gi
	Browse Files					exec run fm
	Open Firefox					exec run www
	Open Chrome					exec chrome
	Chat						exec run hexchat
	Games
		TES III Morrowind			exec run morrowind
		GNU Backgammon				exec run backgammon
		Spider					exec run spider
		Super Tux Kart				exec run supertuxkart
Window
	KEY:A-S-S	Stick container			exec shodc state -y
	KEY:A-S-N	Shade container			exec shodc state -s
	KEY:A-S-B	Lower container			exec shodc state -b
	KEY:A-S-A	Raise container			exec shodc state -a
	KEY:A-S-Z	Minimize container		exec shodc state -m
	KEY:A-S-W	Maximize container		exec shodc state -M
	KEY:A-S-F	FullScreen			exec shodc state -f
	Move to
		KEY:A-S-1	Move to desktop 1	exec shodc sendto 1
		KEY:A-S-2	Move to desktop 2	exec shodc sendto 2
		KEY:A-S-3	Move to desktop 3	exec shodc sendto 3
		KEY:A-S-4	Move to desktop 4	exec shodc sendto 4
		KEY:A-S-5	Move to desktop 5	exec shodc sendto 5
	Focus
		KEY:A-H		Focus column on the left	exec shodc focus -L
		KEY:A-L		Focus column on the right	exec shodc focus -R
		KEY:A-K		Focus row above			exec shodc focus -T
		KEY:A-J		Focus row below			exec shodc focus -B
		KEY:A-P		Focus previous tab		exec shodc focus -cP
		KEY:A-N		Focus next tab			exec shodc focus -cN
	KEY:A-S-Q		Close window			exec shodc close
