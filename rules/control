Windows
	KEY:A-Tab	Focus Previous Window			exec shodc focus -cp
	KEY:A-S-Tab	Focus Next Window			exec shodc focus -cn
	Desktop
		KEY:A-1		Go to desktop 1			exec shodc goto 1
		KEY:A-2		Go to desktop 2			exec shodc goto 2
		KEY:A-3		Go to desktop 3			exec shodc goto 3
		KEY:A-4		Go to desktop 4			exec shodc goto 4
		KEY:A-5		Go to desktop 5			exec shodc goto 5
	PIPE:shodc list -l | awk -F "\t" '$4==$6{print $7 "\t" $6}'	Windows list		exec shodc focus
Music
	Toggle		exec mpc toggle
	Previous	exec mpc cdprev
	Next		exec mpc next
	PIPE:mpc lsplaylists				Playlist	mpc clear && mpc load
	PIPE:mpc playlist | awk '{printf $0 "\t";gsub(/[-.()]/," ");$0=$0;for(i=1;i<=NF;i++)printf " any " $i; print ""}'		Song		exec mpc searchplay
Help
	PIPE:cat $HOME/var/cache/man.1		Section 1	exec xman
	PIPE:cat $HOME/var/cache/man.2		Section 2	exec xman
	PIPE:cat $HOME/var/cache/man.3		Section 3	exec xman
	PIPE:cat $HOME/var/cache/man.4		Section 4	exec xman
	PIPE:cat $HOME/var/cache/man.5		Section 5	exec xman
	PIPE:cat $HOME/var/cache/man.6		Section 6	exec xman
	PIPE:cat $HOME/var/cache/man.7		Section 7	exec xman
	PIPE:cat $HOME/var/cache/man.8		Section 8	exec xman
	PIPE:cat $HOME/var/cache/man.9		Section 9	exec xman
